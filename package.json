{
  "private": true,
  "name": "bring-it",
  "version": "0.0.0",
  "description": "Deployment tool for frontend",
  "license": "MIT",
  "author": {
    "name": "Eric Chen",
    "email": "airkro@qq.com"
  },
  "homepage": "https://github.com/airkro/bring-it",
  "repository": {
