{
  "private": true,
  "name": "bring-it",
  "version": "0.0.0",
  "description": "Deployment tool for frontend",
  "license": "MIT",
  "author": {
    "name": "Eric Chen",
    "email": "airkro@qq.com"
  },
  "homepage": "https://github.com/airkro/bring-it",
  "repository": {
    "type": "git",
    "url": "https://github.com/airkro/bring-it.git"
  },
  "bugs": {
    "url": "https://github.com/airkro/bring-it/issues"
  },
  "scripts": {
    "lint:staged": "nice-move lint staged",
    "prepare": "nice-move git hooks"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@nice-move/cli": "0.11.3",
    "@nice-move/eslint-config-base": "^0.11.3",
    "@nice-move/prettier-config": "^0.11.1",
    "best-shot": "^0.5.2",
    "eslint": "^8.57.0",
    "garou": "^0.7.2",
    "prettier": "^3.2.5"
  },
  "peerDependencies": {
    "@bring-it/npm": "workspace:*",
    "@bring-it/utils": "workspace:*"
  },
  "engines": {
    "node": "^18.0.0 || ^20.0.0",
    "npm": ">=9.4.0"
  },
  "packageManager": "pnpm@8.15.4",
  "pnpm": {
    "overrides": {
      "cpu-features": "npm:cheetor@0.0.0"
    }
  },
  "eslintConfig": {
    "extends": "@nice-move/eslint-config-base"
  },
  "nice-move": {
    "bundle": [
      "packages/**"
    ],
    "internal-regex": "^@bring-it\\/utils"
  },
  "prettier": "@nice-move/prettier-config"
