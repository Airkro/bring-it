    "typescript": "~5.9.2"
  },
  "peerDependencies": {
    "@bring-it/npm": "workspace:^",
    "@bring-it/utils": "workspace:^"
  },
  "packageManager": "pnpm@10.15.1",
  "engines": {
    "node": ">=22.18.0",
    "npm": "^10.5.0",
    "pnpm": "^10.15.0"
